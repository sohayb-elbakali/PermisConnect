        "expo-font",
        {
          "fonts": ["./assets/fonts/FiraSans-MediumItalic.ttf"],
          "android": {
            "fonts": [
              {
                "fontFamily": "Inter",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Inter-BoldItalic.ttf",
                    "weight": 700,
